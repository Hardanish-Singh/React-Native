{
    "expo": {
        "name": "aora",
        "slug": "aora",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/logo.png",
        "scheme": "aora",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/logo.png",
            "resizeMode": "contain",
            "backgroundColor": "#161622"
        },
        "ios": {
            "supportsTablet": true,
            "package": "com.jsm.aora"
        },
        "android": {
            "supportsTablet": true,
            "package": "com.jsm.aora"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/logo.png"
        },
        "plugins": [
            "expo-router",
            "expo-font",
            [
                "expo-video",
                {
                    "supportsBackgroundPlayback": true,
                    "supportsPictureInPicture": true
                }
            ],
            [
                "expo-splash-screen",
                {
                    "ios": {
                        "backgroundColor": "#161622",
                        "image": "./assets/images/logo.png",
                        "resizeMode": "cover"
                    },
                    "android": {
                        "backgroundColor": "#161622",
                        "image": "./assets/images/logo.png",
                        "resizeMode": "cover"
                    }
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        }
    }
}
